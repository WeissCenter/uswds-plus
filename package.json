{
  "name": "uswds-css-custom-props",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build:tokens": "tsc tokens-extractor.ts && node tokens-extractor.js",
    "build:css": "postcss usaemwds.css -o usaemwds.min.css",
    "compile:scss": "sass src/styles.scss usaemwds.css",
    "prestart": "npm run build:tokens",
    "start": "ng serve",
    "prewatch": "npm run build:tokens",
    "build": "ng build && npm run build:tokens && npm run compile:scss && npm run build:css",
    "watch": "concurrently \"npm run watch:tokens\" \"ng build --watch --configuration development\"",
    "test": "ng test",
    "watch:tokens": "nodemon --watch 'src/**/tokens.ts' --exec 'npm run build:tokens'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular/cli": "~16.2.3",
    "@angular/compiler-cli": "^16.2.0",
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@types/glob": "^8.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.11.16",
    "@types/uswds__uswds": "^3.3.3",
    "@uswds/uswds": "^3.7.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "glob": "^10.3.10",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.3",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.5"
  }
}
